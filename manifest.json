{
    "manifest_version": 3,
    "name": "Flight Scraper",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
    {
      "matches": ["*://*.kiwi.com/*", "*://kiwi.com/*", "*://www.azair.eu/*"],
      "js": ["content.js", "utility.js"]
    }
    ]
}
